{
  "name": "radiomag-server-express",
  "private": true,
  "type": "module",
  "main": "src/app.ts",
  "scripts": {
    "predev": "clear && node ../tools/compileEndpoints.js && node ../tools/printRoutes.js",
    "dev": "nodemon ./src/app.ts -e ts",
    "ts-node-config": "ts-node --showConfig",
    "start": "node ./dist/radiomag-server-express/src/app.js",
    "pretests": "rm -rf ../endpoints/nodeEndpoints.js",
    "tests": "(ts-node ./src/app.ts &); jest --silent",
    "posttests": "killall node",
    "build": "chmod +x ./build.sh && ./build.sh",
    "migrate": "knex migrate:latest",
    "migrate-make": "../node_modules/.bin/knex migrate:make $1",
    "migrate-rollback": "knex migrate:rollback",
    "seeds": "knex seed:run",
    "seeds-make": "knex seed:make $1"
  }
}
